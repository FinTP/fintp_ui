		<c:when test="${!empty param.dbtID}">
			<spring:message code="reports.headerDbtid"/>: <b>${param.dbtID}</b>; 
		</c:when>
	</c:choose>
	<br />

	<!-- PAGINATOR -->
<b><label><spring:message code="reports.nbItems"/></label></b>
<select id="itemsPerPage">
	<option>5</option>
	<option>10</option>
	<option>20</option>
	<option>25</option>
	<option>50</option>
	<option selected="selected">100</option>
</select>
<br/>
<b>Total: </b><span id="total">${total}</span>

<div id="paginatorLocation">
	<div id="paginator"/>
</div>

<table class="reportsTable jtable">
		<thead>
			<tr>
				<c:choose>
					<c:when test="${param.insertdateCheck eq 'on'}">
