		</c:when>
	</c:choose>
	
	<c:choose>
		<c:when test="${!empty param.maturityDate}">
			<spring:message code="reports.headerMaturitydate"/>: <b>${param.maturityDate}</b>; 
		</c:when>
	</c:choose>
	
