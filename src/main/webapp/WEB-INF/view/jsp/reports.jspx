		</c:when>
	</c:choose>
	<c:choose>
		<c:when test="${!empty param.batchID}">
			<spring:message code="reports.headerBatchid"/>: <b>${param.batchID}</b>; 
		</c:when>
	</c:choose>
	<c:choose>
		<c:when test="${!empty param.dbtaccount}">
			<spring:message code="reports.headerDbtaccount"/>: <b>${param.dbtaccount}</b>; 
