<html xmlns:c="http://java.sun.com/jsp/jstl/core"  
xmlns:spring="http://www.springframework.org/tags" 
xmlns:tiles="http://tiles.apache.org/tags-tiles" 
xmlns:jsp="http://java.sun.com/JSP/Page">

	<jsp:output doctype-root-element="HTML" doctype-system="about:legacy-compat" />
	<jsp:directive.page contentType="text/html;charset=UTF-8" />  

	<head>
		<meta charset="utf-8"/>
		<spring:url value="/style" var="styleUrl" />
		
		<link href="${styleUrl}/app.css" rel="stylesheet"/>
		<link href="${styleUrl}/content.css" rel="stylesheet"/>
		
		<spring:url value="/scripts/locale.js" var="locale_js_url" />
		<script src="${locale_js_url}" type="text/javascript"><!-- required for FF3 and Opera --></script>

		<spring:url var="jquery_js_url" value="/scripts/jquery/jquery-1.10.1.js" />
		<script src="${jquery_js_url}" type="text/javascript"><!-- required for FF3 and Opera --></script>

		<spring:url var="queues_js_url" value="/scripts/queuesDynamic.js" />
		<script src="${queues_js_url}" type="text/javascript"><!-- required for FF3 and Opera --></script>
			
		<title><c:out value="${title}"/></title>
	</head>

	<body style="margin:2px">
		<div id="container">
			<jsp:include page="/WEB-INF/view/jsp/tiles/header.jspx"/>	
			<tiles:insertAttribute name="main" />
			<jsp:include page="/WEB-INF/view/jsp/tiles/footer.jspx"/>
		</div>
	</body>
</html>