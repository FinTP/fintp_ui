<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:context="http://www.springframework.org/schema/context"
	xmlns:mvc="http://www.springframework.org/schema/mvc"
	xsi:schemaLocation="http://www.springframework.org/schema/mvc http://www.springframework.org/schema/mvc/spring-mvc-3.1.xsd
		http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.1.xsd
		http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-3.0.xsd">

	<bean id="queueDao" class="ro.allevo.fintpui.dao.QueueRestApiDao"></bean>
	<bean id="messagesDao" class="ro.allevo.fintpui.dao.MessagesJdbcDao"></bean>
	<bean id="serviceMapDao" class="ro.allevo.fintpui.dao.ServiceMapRestApiDao"></bean>
	<bean id="timeLimitsDao" class="ro.allevo.fintpui.dao.TimeLimitsRestApiDao"></bean>
	<bean id="routingSchemaDao" class="ro.allevo.fintpui.dao.RoutingSchemaRestApiDao" ></bean>
	<bean id="routingRuleDao" class="ro.allevo.fintpui.dao.RoutingRuleRestApiDao" ></bean>
	<bean id="messageTypesDao" class="ro.allevo.fintpui.dao.MessageTypesRestApiDao" ></bean>
	<bean id="eventDao" class="ro.allevo.fintpui.dao.EventRestApiDao" ></bean>
	<bean id="userDao" class="ro.allevo.fintpui.dao.UserRestApiDao" ></bean>
</beans>